<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ./partials/head %>
</head>
<body class="container">

<header>
    <% include ./partials/header %>
</header>

<main>
    <div class="jumbotron">
        <h1>Kills</h1>
        <% killList.forEach(function(kill) { %>
            <li><%= kill.boat %> - <%= kill.date %> - <%= kill.isLegend %> - <%= kill.isEmpty %></li>
        <% }); %>
    </div>
</main>

<footer>
    <% include ./partials/footer %>
</footer>

</body>
</html>